{
	"env": {
		"browser": true,
		"es2021": true
	},
	"extends": ["eslint:recommended", "plugin:react/recommended", "plugin:@next/next/recommended"],
	"parserOptions": {
		"ecmaFeatures": {
			"jsx": true
		},
		"ecmaVersion": "latest",
		"sourceType": "module"
	},
	"plugins": ["react", "tailwindcss"],
	"rules": {
		"no-unused-vars": "off",
		"no-duplicate-imports": "error",
		"react/prop-types": 0,
		"react/react-in-jsx-scope": 0
	}
}
